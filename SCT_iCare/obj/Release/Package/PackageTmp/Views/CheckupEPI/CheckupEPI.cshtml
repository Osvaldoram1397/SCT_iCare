
@{
    ViewBag.Title = "CheckupEPI";
}

<br />
<h2 class="labelGral">Checkup EPI</h2>
<br />
<h3 class="labelGral">Selecciona el archivo PDF a subir...</h3>

<hr />
<form method="post" enctype="multipart/form-data" action="@Url.Content("~/CheckupEPI/SubirArchivo")">
    

    <div class="form-group labelGral">
        <input type="file" name="file" asp-for="file" class="" required />
        <br />
    </div>

    <input type="submit" value="Subir PDF" class="btn btn-default boton-transparente" />
</form>

