<form method="post" enctype="multipart/form-data" action="@Url.Content("~/ArchivoClinico/Guardar_Dictamen")" style="display:inline-block; margin:0.5%">

    <h4 style="text-align:left; color:white">Nota Médica:</h4>
    <textarea style="width:300vw" name="nota"></textarea>

    <br />
    <br />

    <input type="hidden" value="@idPaciente" name="id" />
    @{
        if (revision1.VascularCerebral != "SI" && revision4.Suenio != "SI" && revision4.Apnea != "SI"
            && revision5.Protesis != "SI" && consulta4.Patologia != "Hipoacusia severa o profunda bilateral" && consulta.Cromatica != "Protanopia")
        {
            <button type="button" class="btn btn-success" data-toggle="modal" data-target="#apto">Concluir examen como APTO</button>
            <button type="button" class="btn btn-warning" data-toggle="modal" data-target="#revaloracion">Enviar a Revaloración</button>
            @*<input type="submit" name="accion" class="btn btn-success" value="Concluir examen como APTO" />
                <input type="submit" name="accion" class="btn btn-warning" value="Enviar a Revaloración" />*@
        }
        else if (revision1.VascularCerebral == "SI" || revision4.Suenio == "SI" || revision4.Apnea == "SI"
            || revision5.Protesis == "SI" || consulta4.Patologia == "Hipoacusia severa o profunda bilateral" || consulta.Cromatica == "Protanopia")
        {
            <button type="button" class="btn btn-danger" data-toggle="modal" data-target="#noapto">Concluir examen como NO APTO</button>
            @*<input type="submit" name="accion" class="btn btn-danger" value="Concluir examen como NO APTO*" />*@
        }
        else
        {
            <button type="button" class="btn btn-danger" data-toggle="modal" data-target="#noapto">Concluir examen como NO APTO</button>
            <button type="button" class="btn btn-warning" data-toggle="modal" data-target="#revaloracion">Enviar a Revaloración</button>
            @*<input type="submit" name="accion" class="btn btn-danger" value="Concluir examen como NO APTO" />
                <input type="submit" name="accion" class="btn btn-warning" value="Enviar a Revaloración" />*@
        }
    }


    @*<button type="button" class="btn btn-danger" data-toggle="modal" data-target="#noapto">Concluir examen como NO APTO</button>*@

