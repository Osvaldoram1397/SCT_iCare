            <h4 style="text-align:left; color:white"><b>CIE-10 (E-11) Diabetes Mellitus Tipo 2 - <span class="text-primary">MEDICINA INTERNA</span></b></h4>
        }

        if ((Convert.ToInt32(consulta2.Frecuencia) < 65 && Convert.ToInt32(consulta2.Frecuencia) > 99))
        {
            <h4 style="text-align:left; color:white"><b>CIE-10 (I45) Otros trastornos de la conducción - <span class="text-primary">CARDIÓLOGO</span></b></h4>
        }

        if (Convert.ToDouble(consulta1.IMC) >= 30
        || Convert.ToInt32(consulta1.Grasa) >= 35)
        {
            <h4 style="text-align:left; color:white"><b>CIE-10 (E66) Obesidad - <span class="text-primary">NUTRICIÓN</span></b></h4>
        }

        if ((Convert.ToDouble(consulta.DerechoAVC) >= 0.7 && Convert.ToDouble(consulta.DerechoAVC) < 1) || (Convert.ToDouble(consulta.DerechoAVM) >= 0.7 && Convert.ToDouble(consulta.DerechoAVM) < 1)
        || (Convert.ToDouble(consulta.DerechoAVL) >= 0.7 && Convert.ToDouble(consulta.DerechoAVL) < 1) || (Convert.ToDouble(consulta.IzquierdoAVC) >= 0.7 && Convert.ToDouble(consulta.IzquierdoAVC) < 1)
        || (Convert.ToDouble(consulta.IzquierdoAVM) >= 0.7 && Convert.ToDouble(consulta.IzquierdoAVM) < 1) || (Convert.ToDouble(consulta.IzquierdoAVL) >= 0.7 && Convert.ToDouble(consulta.IzquierdoAVL) < 1)
        || consulta.CampoVisual != "NORMAL" || consulta.MovimientosOculares != "NORMAL" || consulta.DerechoRP != "NORMAL"
        || consulta.IzquierdoRP != "NORMAL" || consulta.Cromatica == "Ir a SCT")
        {
            <h4 style="text-align:left; color:white"><b>ALTERACION DE LA VISTA NO ESPECIFICADA - <span class="text-primary">OFTALMÓLOGO</span></b></h4>
        }

        if (Convert.ToInt32(consulta.Estereopsis) > 60)
        {
            <h4 style="text-align:left; color:white"><b>CIE-10 (53.32) Estereopsis Defectuosa - <span class="text-primary">OFTALMÓLOGO</span></b></h4>
        }

        if (consulta4.Grafica != "NORMAL")
        {
            <h4 style="text-align:left; color:white"><b>CIE-10 (H-918) OTRAS HIPOACUSIAS ESPECIFICADAS - <span class="text-primary">MEDICINA INTERNA</span></b></h4>
        }

        if (revision1.Cardiopatia == "SI" || revision1.Infarto == "SI" || revision3.Cardiopatias == "SI")
        {
            <h4 style="text-align:left; color:white"><b>I-25 ENFERMEDAD ISQUEMICA CRONICA DEL CORAZON - <span class="text-primary">CARDIÓLOGO</span></b></h4>
        }


        if (revision5.Romberg == "SI" || revision5.PuntaNariz == "SI")
        {
            <h4 style="text-align:left; color:white"><b>ALTERACION NEUROLOGICA NO ESPECIFICADA - <span class="text-primary">MEDICINA INTERNA</span></b></h4>
        }

        if (revision1.Tiroides == "SI")
        {
            <h4 style="text-align:left; color:white"><b>E-079 TRASTORNO DE LA GLANDULA TIROIDES, NO ESPECIFICADO - <span class="text-primary">MEDICINA INTERNA</span></b></h4>
        }

        if (revision5.Estrabismo == "SI")
        {
            <h4 style="text-align:left; color:white"><b>H-509 ESTRABISMO, NO ESPECIFICADO - <span class="text-primary">OFTALMOLOGÍA</span></b></h4>
        }

        if (consulta4.Patologia == "Media unilateral/severa contralateral")
        {
            <h4 style="text-align:left; color:white"><b>H-918 OTRAS HIPOACUSIAS ESPECIFICADAS - <span class="text-primary">MEDICINA INTERNA</span></b></h4>
        }

        <hr />
    }

}
@{
    if (consulta.Cromatica == "Pronatopia" || consulta4.Patologia == "Hipoacusia severa o profunda bilateral" || revision1.VascularCerebral == "SI"
